   "source": [
    "def gcd(a, b):\n",
    "    if a ==0:\n",
    "        return b \n",
    "    elif b == 0:\n",
    "        return a\n",
    "    elif a > b :\n",
    "        return gcd(a - b, b)\n",
    "    else:\n",
    "        return gcd(a, b - a)\n",
    "    \n",
    "def lcm(a, b ):\n",
    "    return a * b / gcd(a, b)\n",
    "\n",
    "a,b = 20 , 8\n",
    "\n",
    "print(\"Least common multple is:\" + str(lcm(a, b )))"
   ]
  }
